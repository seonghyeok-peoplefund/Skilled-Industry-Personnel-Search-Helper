<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.산업기능요원" parent="Theme.MaterialComponents.Light.NoActionBar">

        <item name = "windowActionBar">false</item>
        <item name = "windowNoTitle">true</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">#494949</item>
        <item name="colorOnPrimary">#000000</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/white</item>
        <item name="colorSecondaryVariant">#373737</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@android:color/transparent</item>
        <item name="android:windowLightStatusBar" tools:targetApi="23">true</item>
        <!-- Customize your theme here. -->

        <item name="colorSurface">?attr/colorPrimary</item>
        <item name="backgroundColor">?attr/colorPrimary</item>
        <item name="android:windowBackground">?attr/colorPrimary</item>
        <item name="titleTextColor">?attr/colorOnPrimary</item>


    </style>

    <style name="StrokedText">
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">8</item>
        <item name="android:shadowColor">#FFFFFF</item>
        <item name="android:textColor">#000000</item>
    </style>
</resources>